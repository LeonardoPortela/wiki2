#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@aoberoi/passport-slack/node_modules/needle/bin/needle" "$@"
  ret=$?
else 
  node  "$basedir/../@aoberoi/passport-slack/node_modules/needle/bin/needle" "$@"
  ret=$?
fi
exit $ret
